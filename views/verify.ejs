<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Verify|Teachers</title>
  </head>
  <body>
    <h1>H1</h1>
    <% for(let i = 0; i < desire.length; i++) { %>
    <li id="<%=i%>" onclick="fun(<%=i%>)"><%= desire[i].email %></li>
    <% } %>
    <script>
      const fun = async (id) => {
        const url = "/show/verification";
        const ele = document.getElementById(id).textContent;
        const data = "rad";
        var request = {
          method: "POST",
          body: JSON.stringify({ ele }),
          headers: {
            "Content-Type": "application/json; charset=UTF-8",
          },
        };
        fetch(url, request)
          .then((response) => {
            return window.location.reload(true);
            // return (window.location.href = "/show/verify");
          })
          .catch((err) => console.log(err));
      };
    </script>
  </body>
</html>
